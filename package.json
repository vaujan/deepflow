		"radix-ui": "^1.4.3",
		"react": "19.1.0",
		"react-daisyui-timetools": "^2.0.2",
		"react-dom": "19.1.0",
		"react-markdown": "^10.1.0",
		"react-resizable-panels": "^3.0.6",
		"recharts": "^2.15.4",
		"rehype-katex": "^7.0.1",
		"remark-gfm": "^4.0.1",
		"remark-math": "^6.0.0",

{
	"name": "deepflow.click dev",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev -p 3010",
		"dev:electron": "cross-env ELECTRON_DEV=true NEXT_DEV_URL=http://localhost:3010 concurrently \"pnpm:dev\" \"pnpm:electron\"",
		"electron": "wait-on http://localhost:3010 && electron .",
		"build:electron": "pnpm build && electron-builder -c.extraMetadata.main=electron/main.js",
		"build": "next build",
		"start": "next start",
		"lint": "next lint"
	},
	"dependencies": {

		"@milkdown/prose": "^7.17.1",
		"@milkdown/react": "^7.17.1",
		"@milkdown/theme-nord": "^7.17.1",
		"@radix-ui/colors": "^3.0.0",
		"@radix-ui/react-context-menu": "^2.2.16",
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-dropdown-menu": "^2.1.16",
		"@radix-ui/react-scroll-area": "^1.2.10",
		"@supabase/ssr": "^0.7.0",
		"@tailwindcss/postcss": "^4.1.12",
		"@tanstack/query-sync-storage-persister": "^5.90.2",
		"@tanstack/react-query": "^5.90.2",
		"@tanstack/react-query-persist-client": "^5.90.2",
		"@tanstack/react-table": "^8.21.3",
		"@tanstack/react-virtual": "^3.13.12",
		"@tiptap/extension-code-block": "^3.3.0",
		"@tiptap/extension-hard-break": "^3.3.1",
		"@tiptap/extension-highlight": "^3.3.0",
		"@tiptap/extension-horizontal-rule": "^3.3.0",
		"@tiptap/extension-image": "^3.3.0",
		"@tiptap/extension-link": "^3.3.0",
		"@tiptap/extension-placeholder": "^3.3.0",
